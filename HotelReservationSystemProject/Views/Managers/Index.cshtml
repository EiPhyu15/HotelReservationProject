@model IEnumerable<HotelReservationSystemProject.Models.Manager>

@{
    ViewData["Title"] = "Index";
}

<h1>Manager</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ManagerFName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ManagerLName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ManagerInfo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ManagerFName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ManagerLName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ManagerInfo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ManagerId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ManagerId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ManagerId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
